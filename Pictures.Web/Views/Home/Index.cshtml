@model PicViewModel

@foreach (var pic in Model.pictures)
{
<div class="row">
    <div class="col-md-8 offset-md-2">
        <div class="jumbotron" style="margin-bottom: 20px; text-align: center;">
            <a href="/home/viewimage?id=@pic.Id">
                <h3>@pic.ImageName</h3>
                <h5>Uploaded at: @pic.Date.ToLongDateString()</h5>
                <img style="width: 400px;" src="/Uploads/@pic.FilePath" />
            </a>
        </div>
        </div>
    </div>
        }
